public static Integer findLargest(Integer num1, Integer num2, Integer num3) {
	//code here
    if(num1>num2 && num1>num3)
    {
        return  Integer num1;
    }
    else if(num2>num1 && num2>num3)
    {
        return  Integer num2;
    }
    else if(num3>num1 && num3>num2){
        return   Integer num3;
    }
}