public List<Integer> positiveIntegers(List<Integer> numbers) {
    //code hereList
    List<Integer> Positives=new List<Integer>();
    if(numbers!=null){
        for(Integer i=0;i<numbers.size();i++){
            if(numbers[i]>0){
               Positives.add(numbers[i]);
            }
        }
    }
    return Positives;
}