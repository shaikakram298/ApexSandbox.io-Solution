public List<String> fullNames(List<String> firstNames, List<String> lastNames) {
    //code here
    List<String> fn =  List<String>();
    if(firstNames.size()!=null&&lastNames.size()!=null&&firstNames.size()==lastNames.size()){
    for(integer i=0;i<firstNames.size();i++){
        {
         String full=firstNames[i] + lastNames[i];
         fn.add(full);
        }
    }
    
    return fn;
}