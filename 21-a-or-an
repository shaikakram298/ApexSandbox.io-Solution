public String aOrAn(String word) {
    //code here
   
    if (word == null || word.isEmpty()) {
            return '';
        }
        Char firstLetter = word.toLowerCase().charAt(0);
        String vowels = 'aeiou';
        if (vowels.contains(String.valueOf(firstLetter))) {
            return 'an ' + word;
        } else {
            return 'a ' + word;
        }
    }

}