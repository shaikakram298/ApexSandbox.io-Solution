public Integer maxConsecutiveOnes(Integer[] numbers) {
    //code here
    List<Integer> l=new List<Integer>();
    Integer c=0,n=numbers.size();
    for(Integer i=0;i<n;i++){
        If(numbers[i]!=0)
        {
            c++;
        }
        l.add(c);
        
    }
    l.sort();
    return l[l.size()-1];
}