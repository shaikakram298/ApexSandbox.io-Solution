public Integer maxConsecutiveOnes(Integer[] numbers) {
    //code here
    List<Integer> l=new List<Integer>();
    Integer c=0;
    for(Integer i=0;i<numbers.size();i++){
        If(numbers[i]==1)
        {
            c++;
        }
        l.add(c);
        else if(numbers[i]==0)
        {
                continue;
         }
        
          
    }
   
    l.sort();
    return l[l.size()-1];
}